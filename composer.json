{
    "name": "eisberg/yml-gettext-extractor",
    "description": "The YAML Gettext Extractor is Poedit friendly tool which extracts translations from YML config.",
    "type": "library",
    "require": {
        "symfony/console": "^3.3",
        "symfony/routing":     "~3.0",
        "symfony/filesystem":  "~3.0",
        "symfony/translation": "~3.0",
        "symfony/form":        "~3.0",
        "symfony/asset": "~2.8|~3.0",
        "symfony/yaml": "^3.3"
    },
    "authors": [
        {
            "name": "Alexander Rakushin",
            "email": "rakushinav@modeltech.ru"
        }
    ],
    "require-dev": {
        "phpunit/phpunit":     "~4.8"
    },

    "autoload": {
        "psr-4": { "Yml\\": "./src" }
    },
    "bin": [
        "yml-gettext-extractor"
    ],

    "config": {
        "bin-dir": "bin"
    }
}
